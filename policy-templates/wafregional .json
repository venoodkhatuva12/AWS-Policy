{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "waf-regional:GetChangeTokenStatus",
                "waf-regional:List*",
                "waf-regional:GetChangeToken"
                
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "waf-regional:*",
            "Resource": [
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:geomatchset/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:ipset/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:xssmatchset/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:ratebasedrule/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:regexmatchset/*",
                "arn:aws:elasticloadbalancing:REGION_HERE:ACCOUNT_NUMBER_HERE:loadbalancer/app/COMPONENT_NAME_HERE*/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:sizeconstraintset/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:webacl/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:sqlinjectionmatchset/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:regexpatternset/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:bytematchset/*",
                "arn:aws:waf-regional:REGION_HERE:ACCOUNT_NUMBER_HERE:rule/*"
            ]
        }
    ]
}