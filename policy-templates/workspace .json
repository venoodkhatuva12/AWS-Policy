{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "workspaces:Describe*",
                "workspaces:DeleteTags",
                "workspaces:CreateTags"
               
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "workspaces:*",
            "Resource": "arn:aws:sky:REGION_HERE:ACCOUNT_NUMBER_HERE:workspacebundle/*"
        }
    ]
}